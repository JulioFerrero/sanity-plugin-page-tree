{
  "name": "@julioferrero/sanity-plugin-page-tree",
  "version": "2.1.1",
  "description": "A Sanity plugin to make managing nested pages in a tree easier",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "type": "module",
  "license": "MIT",
  "author": "Djovanni Tehubijuluw <djovanni@q42.nl>",
  "contributors": [
    {
      "name": "Tobias Stikvoort",
      "email": "tobias@q42.nl"
    },
    {
      "name": "Djovanni Tehubijuluw",
      "email": "djovanni@q42.nl"
    },
    {
      "name": "Stijn de Ligt",
      "email": "stijn@q42.nl"
    }
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./client": {
      "source": "./src/client.ts",
      "import": "./dist/client.js",
      "require": "./dist/client.cjs",
      "default": "./dist/client.js"
    },
    "./next": {
      "source": "./src/next.ts",
      "import": "./dist/next.js",
      "require": "./dist/next.cjs",
      "default": "./dist/next.js"
    },
    "./presentation-mode": {
      "source": "./src/presentation-mode.ts",
      "import": "./dist/presentation-mode.js",
      "require": "./dist/presentation-mode.cjs",
      "default": "./dist/presentation-mode.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "client.js",
    "v2-incompatible.js"
  ],
  "sideEffects": false,
  "browserslist": "extends @sanity/browserslist-config",
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "clean": "rimraf dist",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint ./src --max-warnings 0",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^3.1.11",
    "@sanity/uuid": "^3.0.2",
    "lodash": "^4.17.21",
    "next-sanity": "^12.0.14",
    "rxjs": "^7.8.2",
    "sanity-plugin-utils": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@sanity/pkg-utils": "^10.3.4",
    "@sanity/plugin-kit": "^4.0.20",
    "@types/lodash": "^4.17.23",
    "@types/react": "^19.2.10",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "prettier-plugin-packagejson": "^3.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rimraf": "^6.1.2",
    "sanity": "^5.7.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "sanity": "^4 || ^5",
    "styled-components": "^5 || ^6"
  },
  "volta": {
    "node": "22.15.1"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false,
      "scripts": false,
      "nodeEngine": false,
      "tsconfig": false
    }
  }
}
