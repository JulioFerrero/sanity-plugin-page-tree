{
  "name": "@julioferrero/sanity-plugin-page-tree",
  "version": "1.7.0",
  "description": "A Sanity plugin to make managing nested pages in a tree easier",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "author": "Djovanni Tehubijuluw <djovanni@q42.nl>",
  "contributors": [
    {
      "name": "Tobias Stikvoort",
      "email": "tobias@q42.nl"
    },
    {
      "name": "Djovanni Tehubijuluw",
      "email": "djovanni@q42.nl"
    },
    {
      "name": "Stijn de Ligt",
      "email": "stijn@q42.nl"
    }
  ],
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.cjs.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.cjs.mjs"
    },
    "./client": {
      "source": "./src/client.ts",
      "import": "./dist/client.cjs.mjs",
      "require": "./dist/client.js",
      "default": "./dist/client.cjs.mjs"
    },
    "./next": {
      "source": "./src/next.ts",
      "import": "./dist/next.cjs.mjs",
      "require": "./dist/next.js",
      "default": "./dist/next.cjs.mjs"
    },
    "./presentation-mode": {
      "source": "./src/presentation-mode.ts",
      "import": "./dist/presentation-mode.cjs.mjs",
      "require": "./dist/presentation-mode.js",
      "default": "./dist/presentation-mode.cjs.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.cjs.mjs",
  
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "client.js",
    "v2-incompatible.js"
  ],
  "sideEffects": false,
  "browserslist": "extends @sanity/browserslist-config",
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --clean",
    "clean": "rimraf dist",
    "test": "TZ=UTC vitest run",
    "test:watch": "TZ=UTC vitest",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint ./src/** --ext=.ts,.tsx --max-warnings 0",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict",
    "tsc": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "prepare": "npm run build"
  },
  "type": "commonjs",
  "dependencies": {
    "@sanity/icons": "3.7.4",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "3.0.7",
    "@sanity/uuid": "3.0.2",
    "lodash": "4.17.21",
    "next-sanity": "10.0.12",
    "rxjs": "7.8.2",
    "sanity-plugin-utils": "1.6.8"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "7.11.9",
    "@sanity/plugin-kit": "4.0.19",
    "@types/lodash": "4.17.20",
    "@types/react": "19.1.10",
    "@types/styled-components": "5.1.34",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitejs/plugin-react": "5.0.0",
    "eslint": "9.33.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-config-sanity": "7.1.4",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-is": "19.1.1",
    "rimraf": "6.0.1",
    "sanity": "4.4.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "sanity": "^3 || ^4",
    "styled-components": "^5 || ^6"
  },
  "volta": {
    "node": "22.15.1"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false,
      "scripts": false,
      "nodeEngine": false,
      "tsconfig": false
    }
  }
}
